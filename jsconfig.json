// 配置别名  作用是让 vscode 识别别名
{
  "compilerOptions": {
    "baseUrl": "packages",
    "paths": {
      "react/*": ["react/*"],
      "react-dom/*": ["react-dom/*"],
      "react-dom-bindings/*": ["react-dom-bindings/*"],
      "react-reconciler/*": ["react-reconciler/*"],
      "scheduler/*": ["scheduler/*"],
      "shared/*": ["shared/*"]
    }
  },
  "exclude": ["node_modules"]
}
